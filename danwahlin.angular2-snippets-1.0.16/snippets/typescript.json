      "",
      "\tcanDeactivate(",
      "\t\tcomponent: ${ComponentToGuard},",
      "\t\troute: ActivatedRouteSnapshot,",
      "\t\tstate: RouterStateSnapshot",
      "\t) : Observable<boolean> | Promise<boolean> | boolean {",
      "\t\t$0",
      "\t}",
      "",
      "}"
    ],
    "description": "Angular CanDeactivate Routing Guard"
  }, 
  

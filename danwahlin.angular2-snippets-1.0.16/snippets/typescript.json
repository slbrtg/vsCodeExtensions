      "})",
      "export class ${ComponentName}Component implements OnInit {",
      "$0",
      "\tconstructor() { }",
      "",
      "\tngOnInit() { ",
      "",
      "\t}",
      "",
      "}"
    ],
    "description": "Angular component snippet"
  }, 
  
